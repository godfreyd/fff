{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Server side rendering",
  "main": "src/index.ts",
  "scripts": {
    "build:clean": "rm -rf build",
    "build:webpack": "NODE_ENV=development webpack --config ./webpack/webpack.config.ts",
    "build": "npm-run-all build:clean build:webpack",
    "start": "BABEL_ENV=node nodemon --watch build --exec \"node build/bundle.js\"",
    "start:dev": "BABEL_ENV=node nodemon --watch build --exec \"node --trace-warnings build/bundle.js\"",
    "watch:ssr": "webpack --watch --mode=development --config webpack/webpack.config.ts",
    "dev": "npm-run-all --parallel --print-name start:dev watch:ssr",
    "eslint": "npx eslint '**/*.{js,jsx,ts, tsx}' --fix",
    "format:css": "stylelint './src/**/**/*.{scss,css}' --fix",
    "format:js": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "author": "Sergey Bocharov",
  "engines": {
    "node": "14.x"
  },
